{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmSize": {
            "type": "string",
            "defaultValue": "Standard_D4s_v3",
            "allowedValues": [
                "Standard_E64is_v3",
                "Standard_E64i_v3",
                "Standard_M128ms",
                "Standard_GS5",
                "Standard_G5",
                "Standard_DS15_v2",
                "Standard_D15_v2",
                "Standard_F72s_v2",
                "Standard_L32s"
            ],
            "metadata": {
                "description": "VM size of the virtual machine."
            }
        },
        "vmName": {
            "type": "string",
            "defaultValue": "devops",
            "minLength": 3,
            "maxLength": 15,
            "metadata": {
                "description": "Name of the the virtual machine."
            }
        },
        "adminUsername": {
            "type": "string",
            "minLength": 1,
            "metadata": {
                "description": "Name of the local administrator account, this cannot be Admin, Administrator or root."
            }
        },
        "adminPassword": {
            "type": "securestring",
            "minLength": 15,
            "metadata": {
                "description": "Local administrator password, complex password is required, do not use any variation of the password word because it will be rejected."
            }
        },
        "storageAccountName": {
            "type": "string",
            "metadata": {
                "description": "Name of the Storage Account to be used for archiving logs and metrics."
            }
        }
    },
    "variables": {  },
    "functions": [  ],
    "resources": [  ],
    "outputs": {  }
}